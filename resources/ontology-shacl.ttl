###  http://data.quality-link.eu/ontology/v1#registrationPortal
ql:registrationPortal rdf:type owl:DatatypeProperty ;
                      rdfs:range xsd:anyURI ;
                      rdfs:comment "A web page at which one can register or enrol for a Learning Opportunity." ;
                      rdfs:label "registration portal"@en .


###  http://data.quality-link.eu/ontology/v1#version
ql:version rdf:type owl:DatatypeProperty ;
    rdfs:range xsd:string ;
    rdfs:comment "A number or other identifier that indicates the version of the Learning Opportunity specifcation." ;
    rdfs:label "version"@en .


###  http://data.quality-link.eu/ontology/v1#isActive
ql:isActive a owl:DatatypeProperty ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Learning Opportunity Specification is active, i.e. instances are currently offered or the education provider expects that instances will still be offered in the future." ;
    rdfs:label "is active"@en .


###  http://data.quality-link.eu/ontology/v1#isOpenToNonEnrolled
ql:isOpenToNonEnrolled a owl:DatatypeProperty ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Indicates that a learning opportunity is alos open to learners not currently enrolled as regular students." ;
    rdfs:label "open to non-enrolled learners"@en .


###  http://data.quality-link.eu/ontology/v1#enrolmentCapacity
ql:enrolmentCapacity a owl:DatatypeProperty ;
    rdfs:range xsd:nonNegativeInteger ;
    rdfs:comment "The number of learners that can register for a Learning Opportunity Instance." ;
    rdfs:label "enrolment capacity"@en .


###  http://data.quality-link.eu/ontology/v1#enrolledLearnerCount
ql:enrolledLearnerCount a owl:DatatypeProperty ;
    rdfs:range xsd:nonNegativeInteger ;
    rdfs:comment "The number of learners that are currently enrolled for the Learning Opportunity Instance." ;
    rdfs:label "enrolled learner count"@en .


###  http://data.quality-link.eu/ontology/v1#enrolmentMinimum
ql:enrolmentMinimum a owl:DatatypeProperty ;
    rdfs:range xsd:nonNegativeInteger ;
    rdfs:comment "The number of learners that at least need to enrole for a Learning Opportunity Instance to take place." ;
    rdfs:label "minimum enrolment"@en .


###  http://data.quality-link.eu/ontology/v1#passingRate
ql:passingRate a owl:DatatypeProperty ;
    rdfs:range xsd:positiveInteger ;
    rdfs:label "passing rate"@en ;
    rdfs:comment "The percentage of learners who pass on average."@en .

###  http://data.quality-link.eu/ontology/v1#gradeOrder
ql:gradeOrder a owl:DatatypeProperty ;
    rdfs:range xsd:positiveInteger ;
    rdfs:label "order"@en ;
    rdfs:comment "A number specifying the order of the grade within the grading scheme, the highest number indicating the best grade."@en .

###  http://data.quality-link.eu/ontology/v1#gradePercentage
ql:gradePercentage a owl:DatatypeProperty ;
    rdfs:range xsd:positiveInteger ;
    rdfs:label "percentage"@en ;
    rdfs:comment "The percentage of learners earning this grade."@en .


