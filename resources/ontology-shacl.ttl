    sh:order 11 ;
    sh:property [
        sh:name "provided by"@en ;
        sh:description "Higher Education Institution that offers the LO."@en ;
        sh:class ql:HigherEducationInstitution ;
        sh:minCount 1;
        sh:maxCount 1;
        sh:path elm:providedBy ;
        sh:order 1 ;
    ], [
        sh:name "(local) identifier"@en ;
        sh:description "Identifier of the Learning Opportunity instance. Must be unique within the scope of the institution identified with providedBy."@en ;
        sh:class elm:Identifier ;
        sh:minCount 1;
        sh:maxCount 1;
        sh:path adms:identifier ;
        sh:order 2 ;
    ] .

###  http://data.quality-link.eu/ontology/v1#GradingSchemeSchape
ql:GradingSchemeSchape
    a sh:NodeShape ;
    sh:targetClass elm:GradingScheme ;
    rdfs:label "Grading Scheme"@en ;
    rdfs:comment "Information on the grading scheme used."@en ;
    sh:order 40 ;
    sh:property [
        sh:name "title"@en ;
        sh:description "The title of the grading scheme. One value per language is permitted."@en ;
        sh:uniqueLang true ;
        sh:datatype rdf:langString ;
        sh:path dcterms:title ;
        sh:order 10 ;
    ], [
        sh:name "identifier"@en ;
        sh:description "An identifier of the grading scheme."@en ;
        sh:class elm:Identifier ;
        sh:path adms:identifier ;
        sh:order 1 ;
    ], [
        sh:name "description"@en ;
        sh:description "A free text description of the grading scheme."@en ;
        sh:uniqueLang true ;
        sh:datatype rdf:langString ;
        sh:path dcterms:description ;
        sh:order 12 ;
    ], [
        sh:name "grade distribution"@en ;
        sh:description "Information on the statistical distribution of grades in the scheme."@en ;
        sh:class elm:ResultDistribution ;
        sh:path elm:resultDistribution ;
        sh:order 20 ;
    ], [
        sh:name "other documents"@en ;
        sh:description "A public web document containing additional documentation about the grading scheme."@en ;
        sh:class elm:WebResource ;
        sh:path elm:supplementaryDocument ;
        sh:order 14 ;
    ] .

###  http://data.quality-link.eu/ontology/v1#GradeDistributionShape
ql:GradeDistributionShape
    a sh:NodeShape ;
    sh:targetClass elm:ResultDistribution ;
    rdfs:label "Grade Distribution "@en ;
    rdfs:comment "It describes a histogram of grades achieved by learners within a specific reference group."@en ;
    sh:order 42 ;
    sh:property [
        sh:class ql:LearnerGroup ;
        sh:path ql:referenceGroup ;
        sh:maxCount 1 ;
        sh:order 10 ;
    ], [
        sh:name "grade"@en ;
        sh:description "One specific grade in the grading scheme with the distribution information."@en ;
        sh:class elm:ResultCategory ;
        sh:path elm:resultCategory ;
        sh:minCount 1 ;
        sh:order 20 ;
    ] .

###  http://data.quality-link.eu/ontology/v1#GradeShape
ql:GradeShape
    a sh:NodeShape ;
    sh:targetClass elm:ResultCategory ;
    rdfs:label "Grade"@en ;
    rdfs:comment "Description of a single grade within a histogram of results."@en ;
    sh:order 44 ;
    sh:property [
        sh:name "label"@en ;
        sh:description "The label of the grade according to the grading scheme it is part of, e.g. 'C', '2+' or '10'."@en ;
        sh:datatype xsd:string ;
        sh:path elm:label ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:order 10 ;
    ], [
        sh:datatype xsd:positiveInteger ;
        sh:path ql:gradeOrder ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:order 20 ;
    ], [
        sh:datatype xsd:positiveInteger ;
        sh:path ql:gradePercentage ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:order 30 ;
    ] .

