###  http://data.quality-link.eu/ontology/v1#LearningOpportunitySpecificationReference
ql:LearningOpportunitySpecificationReference
    a owl:Class, sh:NodeShape ;
    rdfs:label "Learning Opportunity Specification Reference"@en ;
    rdfs:comment "A reference to another Learning Opportunity specification, identified by a provider and a (local) identifier, without further details."@en ;
    sh:order 2 ;
    sh:property [
        sh:name "provided by"@en ;
        sh:description "Higher Education Institution that offers the LO."@en ;
        sh:class ql:HigherEducationInstitution ;
        sh:minCount 1;
        sh:maxCount 1;
        sh:path elm:providedBy ;
        sh:order 1 ;
    ], [
        sh:name "(local) identifier"@en ;
        sh:description "Identifier of the Learning Opportunity Specification. Must be unique within the scope of the institution identified with providedBy."@en ;
        sh:class elm:Identifier ;
        sh:minCount 1;
        sh:maxCount 1;
        sh:path adms:identifier ;
        sh:order 2 ;
    ] .

###  http://data.quality-link.eu/ontology/v1#LearningOpportunityInstanceReference
ql:LearningOpportunityInstanceReference
    a owl:Class, sh:NodeShape ;
    rdfs:label "Learning Opportunity Instance Reference"@en ;
