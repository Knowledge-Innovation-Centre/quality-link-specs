#################################################################
#    Object Properties
#################################################################

###  http://data.quality-link.eu/ontology/v1#relatedTo
ql:relatedTo rdf:type owl:ObjectProperty ,
                      owl:AsymmetricProperty ;
             rdfs:comment "A relationship between Learning Opportunities or Organisations (abstract)"@en ;
             rdfs:label "related to"@en .


###  http://data.quality-link.eu/ontology/v1#spunOffFrom
ql:spunOffFrom rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf ql:relatedTo ;
               rdfs:domain elm:Organisation ;
               rdfs:comment "Organisation spun off from another one (i.e. subject newly created by the event, object existed before and after)"@en ;
               rdfs:label "spun off from"@en .


###  http://data.quality-link.eu/ontology/v1#absorbed
ql:absorbed rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf ql:relatedTo ;
            rdfs:domain elm:Organisation ;
            rdfs:comment "Organisation absorbed another one (i.e. subject existed before and after the event, object ceased to exist)"@en ;
            rdfs:label "absorbed"@en .


###  http://data.quality-link.eu/ontology/v1#succeeded
ql:succeeded rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf ql:relatedTo ;
             rdfs:domain elm:Organisation ;
             rdfs:comment "Organisation succeeded/replaced another one (i.e. subject was newly created by the event, object ceased to exist)"@en ;
             rdfs:label "succeeded"@en .


###  http://data.quality-link.eu/ontology/v1#superseded
ql:superseded rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf ql:relatedTo ;
    rdfs:domain ql:LearningOpportunitySpecification ;
    rdfs:comment "Learning Opportunity specification superseded another one, which is no longer (or temporarily not) offered."@en ;
    rdfs:label "superseded"@en .


###  http://data.quality-link.eu/ontology/v1#learnerGroupConditions
ql:learnerGroupConditions rdf:type owl:ObjectProperty ;
               rdfs:domain ql:LearningOpportunityInstance ;
               rdfs:range ql:LearnerGroupConditions ;
               rdfs:comment "Indicates specific conditions that apply to a certain target group of learners" ;
               rdfs:label "specific conditions for learner group"@en .


###  http://data.quality-link.eu/ontology/v1#learnerGroup
ql:learnerGroup rdf:type owl:ObjectProperty ;
               rdfs:domain ql:LearningOpportunityInstance ;
               rdfs:range ql:LearnerGroup ;
               rdfs:comment "Definition of the specific group of learners to whom these conditions apply" ;
               rdfs:label "learner group"@en .


###  http://data.quality-link.eu/ontology/v1#studentsOf
ql:studentsOf rdf:type owl:ObjectProperty ;
              rdfs:domain ql:LearnerGroup ;
              rdfs:comment "Defines that the target group of learners includes the students of an education institution, or an alliance thereof"@en ;
              rdfs:label "includes students of"@en .


###  http://data.quality-link.eu/ontology/v1#limitLearningOpportunity
ql:limitLearningOpportunity rdf:type owl:ObjectProperty ;
    rdfs:domain ql:LearnerGroup ;
    rdfs:comment "Defines that the learning opportunity is offered to students enrolled in a specified other learning opportunity."@en ;
    rdfs:label "specific learning opportunity"@en .


###  http://data.quality-link.eu/ontology/v1#minimumEQFLevel
ql:minimumEQFLevel rdf:type owl:ObjectProperty ;
    rdfs:domain ql:LearnerGroup ;
    rdfs:comment "Defines that the learning opportunity is offered to students enrolled in another learning opportunity at a specified EQF level or higher. If provided, the value must come from the EU controlled vocabulary http://publications.europa.eu/resource/dataset/european-qualification-framework"@en ;
    rdfs:label "minimum EQF Level"@en .


###  http://data.quality-link.eu/ontology/v1#entryRequirementLearningOpportunity
ql:entryRequirementLearningOpportunity rdf:type owl:ObjectProperty ;
    rdfs:range ql:LearningOpportunitySpecificationReference ;
    rdfs:comment "Specific entry requirement or prerequisite to have completed another learning opportunity."@en ;
    rdfs:label "required learning opportunity for entry"@en .


###  http://data.quality-link.eu/ontology/v1#entryRequirementLearningOutcome
ql:entryRequirementLearningOutcome rdf:type owl:ObjectProperty ;
    rdfs:range elm:LearningOutcome ;
    rdfs:comment "Specific entry requirement or prerequisite to have achieved a particular learning outcome."@en ;
    rdfs:label "required learning outcome for entry"@en .


###  http://data.quality-link.eu/ontology/v1#referenceGroup
ql:referenceGroup
    a owl:ObjectProperty ;
    rdfs:domain elm:ResultDistribution ;
    rdfs:range ql:LearnerGroup ;
    rdfs:label "reference group"@en ;
    rdfs:comment "The group of learners for which the histogram of results was established."@en .


###  http://data.quality-link.eu/ontology/v1#virtualLearningEnvironment
ql:virtualLearningEnvironment
    a owl:ObjectProperty ;
    rdfs:domain ql:LearningOpportunityInstance ;
    rdfs:label "virtual learning environment"@en ;
    rdfs:comment "Specifies (a) virtual learning environment(s) used in the delivery of this learning opportunity."@en .


###  http://data.quality-link.eu/ontology/v1#isMemberOf
ql:isMemberOf
    a owl:ObjectProperty ;
    rdfs:label "is member of"@en ;
    rdfs:comment "Specifies that an organisation is member of an association, e.g. an institution member of EUA or EURASHE."@en .


